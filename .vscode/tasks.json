{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C: cc build active file",
      "command": "/usr/bin/cc",
      "args": [
        "${workspaceFolder}/${workspaceFolderBasename}.a",
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}.o"
      ],
      "options": { "cwd": "${fileDirname}" },
      "problemMatcher": ["$gcc"],
      "group": { "kind": "build", "isDefault": true },
      "detail": "Task generated by Debugger."
    }
  ]
}